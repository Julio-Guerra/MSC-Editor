msc failure; 
inst Initiator;
inst Responder; 
gate out ICONreq to Initator;
gate in IDISind from Initator;

Initiator: instance;
condition when Disconnected shared all;
in ICONreq from env;
starttimer T;
out ICON to lost Responder;
condition Wait_For_Resp shared all;
timeout T;
out IDISinf to env;
condition Disconnected shared all;
endinstance;
Responder: instance;
condition when Disconnected shared all;
in ICON from Initiator;
condition Wait_For_Resp shared all;
condition Disconnected shared all;
endinstance;
endmsc; 
